{
  "ansible.python.interpreterPath": "/usr/bin/python3",
  "window.newWindowDimensions": "inherit",
  "window.restoreWindows": "folders",
  "window.zoomLevel": 0,
  "window.density.editorTabHeight": "compact",
  "window.density.editorTabSizing": "shrink",
  "window.density.editorGroupHeaderHeight": "compact",
  "window.density.activityBarWidth": "compact",
  "window.density.sideBarWidth": "compact",
  "window.density.panelHeight": "compact",
  "window.density.statusBarHeight": "compact",
  "window.density.titleBarHeight": "compact",
  "window.density.menuBarHeight": "compact",
  "cSpell.customDictionaries": {
    "custom-dictionary-workspace": {
      "name": "custom-dictionary-workspace",
      "path": "${workspaceFolder:workspace}/.cspell/custom-dictionary-workspace.txt",
      "addWords": true,
      "scope": "workspace"
    }
  },
  "cSpell.words": [
    "ansible",
    "playbook",
    "vars",
    "venv",
    "virtualenv",
    "yaml",
    "yml"
  ],
  "files.exclude": {
    ".pytest_cache": true,
    ".coverage": true,
    "htmlcov": true,
    ".tox": true,
    "**/*.retry": true
  },
  "taskExplorer.exclude": [
    "**/.venv/**",
    "**/node_modules/**"
  ],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.highlights.customHighlight": {
    "BUG": {
      "foreground": "#FF0000",
      "icon": "bug"
    },
    "FIXME": {
      "foreground": "#FF6600",
      "icon": "tools"
    },
    "TODO": {
      "foreground": "#00FF00",
      "icon": "checklist"
    }
  },
  "editor.formatOnSave": true,
  "[markdown]": {
    "editor.defaultFormatter": "fcrespo82.markdown-table-formatter",
    "editor.formatOnSave": true,
    "editor.rulers": [
      80
    ],
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "shellformat.path": "/usr/local/bin/shfmt"
  },
  "yaml.schemas": {
    "file:///workspace/config/ansible/schemas/role.json": [
      "**/roles/*/meta/main.yml"
    ],
    "file:///workspace/config/ansible/schemas/ansible.json": [
      "**/tasks/*.yml",
      "**/handlers/*.yml"
    ],
    "file:///workspace/config/ansible/schemas/playbook.json": [
      "**/playbooks/*.yml",
      "**/site.yml"
    ],
    "file:///workspace/config/ansible/schemas/inventory.json": [
      "**/inventory/*.yml"
    ]
  }
}
